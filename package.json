{
  "name": "express-test",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=6 <7"
  },
  "scripts": {
    "test": "npm run test:unit && npm run test:integration",
    "start": "node ./src/index.js",
    "start:dev": "concurrently --kill-others \"tsc --watch\" \"nodemon --debug ./src/index.js\"",
    "build": "tsc",
    "watch": "tsc --watch",
    "test:unit": "tsc && NODE_ENV=TEST mocha --recursive --require reflect-metadata --require source-map-support/register test/unit",
    "test:integration": "tsc && NODE_ENV=TEST mocha --recursive --require reflect-metadata --require source-map-support/register test/integration"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^1.0.2",
    "body-parser": "^1.16.0",
    "class-transformer": "^0.1.6",
    "connect-sqlite3": "^0.9.9",
    "cors": "^2.8.1",
    "express": "^4.14.0",
    "express-session": "^1.14.2",
    "multer": "^1.2.1",
    "reflect-metadata": "^0.1.9",
    "routing-controllers": "^0.6.9",
    "socket.io": "^1.7.2",
    "sqlite3": "^3.1.8",
    "typedi": "^0.4.3",
    "typeorm": "0.0.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^1.0.0",
    "@types/body-parser": "0.0.33",
    "@types/chai": "^3.4.34",
    "@types/cors": "^2.8.0",
    "@types/express": "^4.0.35",
    "@types/express-session": "0.0.32",
    "@types/mocha": "^2.2.38",
    "@types/node": "^7.0.0",
    "@types/socket.io": "^1.4.27",
    "@types/supertest": "^2.0.0",
    "chai": "^3.5.0",
    "concurrently": "^3.1.0",
    "mocha": "^3.2.0",
    "node-mocks-http": "^1.5.6",
    "nodemon": "^1.11.0",
    "source-map-support": "^0.4.11",
    "supertest": "^2.0.1",
    "typescript": "^2.1.5"
  }
}
