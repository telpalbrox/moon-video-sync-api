    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.1",
    "@types/socket.io": "^1.4.29",
    "@types/supertest": "^2.0.0",
    "chai": "^4.1.2",
    "concurrently": "^3.5.0",
    "mocha": "^4.0.0",
    "node-mocks-http": "^1.6.1",
    "nodemon": "^1.12.0",
