    "connect-redis": "^3.2.0",
    "connect-sqlite3": "^0.9.9",
    "cors": "^2.8.3",
    "express": "^4.16.0",
    "express-session": "^1.15.2",
    "multer": "^1.3.0",
    "parse-database-url": "^0.3.0",
    "pg": "^7.0.0",
    "reflect-metadata": "^0.1.10",
